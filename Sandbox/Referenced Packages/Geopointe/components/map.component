                                <input type="button" class="gp-btn btn-lg btn-danger modalButtonRight" value="{!$Label.Delete}" ng-click="deleteDataSet(selectedDataSet.dataSetIdentifier)"/>
                                <div class="clear"/>
                            </div>
                        </div>

                        <!-- New data Set modal, this is hidden -->
