OR(Survey_Question_Col_ID__r.Position__c -1 ==7,Survey_Question_Col_ID__r.Position__c -1 ==8)
,0,if(
OR(Survey_Question_Col_ID__r.Position__c -1 ==9,Survey_Question_Col_ID__r.Position__c -1 ==10),1,null))),null)</formula>
